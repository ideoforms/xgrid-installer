<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE plist PUBLIC "-//Apple Computer//DTD PLIST 1.0//EN" "http://www.apple.com/DTDs/PropertyList-1.0.dtd">
<plist version="1.0">
<dict>
    <!-- Connect to the first zeroconf discovered controller? -->
	<key>BindToFirstAvailable</key>
	<true/>
    <!-- Only run agent when the computer is idle? -->
	<key>OnlyWhenIdle</key>
	<true/>
    <!-- Authentication method to use with Controller. One of 'Password', 'Kerberos', 'None' -->
	<key>ControllerAuthentication</key>
	<string>Password</string>
    <!-- Controller name to connect to (Either NetService name or host name or IP address) -->
	<key>ControllerName</key>
	<string>dragonfly.gold.ac.uk</string>
    <!-- Should the worker task be suspended when the agent becomes not idle? -->
	<key>SuspendWhenNotIdle</key>
	<false/>
    <!-- Use zero config NetServices to resolve Controller name ? -->
	<key>ResolveNameAsNetService</key>
	<true/>

    <!--

        Other Keys:

	<key>MaximumTaskCount</key>
	<integer>2</integer>

	<key>AgentName</key>
	<string>Tester</string>

	<key>LocalOnly</key>
	<false/>

    -->
</dict>
</plist>
